{
  "name": "lost-wirepuller-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "deploy:manual": "npm run build && gcloud run deploy lost-wirepuller-app --source .",
    "docker:build": "docker build -t gcr.io/$PROJECT_ID/lost-wirepuller-app .",
    "docker:push": "docker push gcr.io/$PROJECT_ID/lost-wirepuller-app",
    "docker:deploy": "gcloud run deploy lost-wirepuller-app --image gcr.io/$PROJECT_ID/lost-wirepuller-app --region us-central1 --platform managed --allow-unauthenticated",
    "logs": "gcloud run services logs read lost-wirepuller-app --region=us-central1"
  },
  "dependencies": {
    "firebase": "^12.1.0",
    "leaflet": "^1.9.4",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-leaflet": "^5.0.0",
    "react-router-dom": "^7.8.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react-swc": "^4.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.12",
    "vite": "^7.1.2"
  }
}
